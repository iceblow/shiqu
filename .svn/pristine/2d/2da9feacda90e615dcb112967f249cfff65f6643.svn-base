#content {
    width: 96%;
    margin: 0 auto;
}
.tablehead {
    background: #e5e6ea;
    color: #464c69;
    font-weight: bold;
    line-height: 47px;
}
.tablethcss {
    color: #333333;
    font-weight: lighter;
}

.border_right {
    border-right: 1px solid #ddd;
}
table th {
    text-align: center;
}

.delbutton {
    width: 85px;
    height: 40px;
    line-height: 40px;
    text-align: center;
    color: #fff;
    background: #f83a22;
    cursor: pointer;
    float: left;
}


.addbutton {
    width: 85px;
    height: 40px;
    line-height: 40px;
    text-align: center;
    color: #fff;
    background: #35aa47;
    cursor: pointer;
    float: left;
}
.table_del_button {
    float: left;
    width: 56px;
    height: 26px;
    cursor: pointer;
}
.table_button_del_icon {
    width: 24px;
    height: 100%;
    float: left;
    background-image: url(../imgs/delicon.png);
    background-repeat: no-repeat;
    background-size: 9px 12px;
    background-position: center;
}
.table_button_text {
    float: left;
    text-align: center;
    line-height: 26px;
    font-size: 12px;
}
.table_editor_button {
    float: left;
    width: 56px;
    height: 26px;
    cursor: pointer;
}
.table_button_editor_icon {
    width: 24px;
    height: 100%;
    float: left;
    background-image: url(../imgs/editoricon.png);
    background-repeat: no-repeat;
    background-size: 9px 12px;
    background-position: center;
}

table tr {
    height: 47px;
}

.form-control {
    border-radius: 0px;
}

.detail_submit {
    width: 100px;
    height: 40px;
    background-color: #6097fb;
    border: none;
    border-radius: 0px;
    background-repeat: no-repeat;
    background-position: 5px;
}

.searchbutton {
    width: 54px;
    height: 40px;
    float: right;
    cursor: pointer;
    background: url(../imgs/searchicon.png);
    background-position: center;
    background-repeat: no-repeat;
}

.time_select {
	float:right;
	padding-left:5px;
    height: 39px;
    border: 1px solid #bdc6d7;
    line-height: 36px;
    margin-right:20px;
    background: url(../imgs/time_input.png);
    background-position: 97px 11px;
    background-repeat: no-repeat;
}

.category {
    background-color: #ffffff;
    background-image: none !important;
    filter: none !important;
    border: 1px solid #bdc6d7;
    outline: none;
    width: 134px !important;
    line-height: 39px;
    height: 39px !important;
    padding-left:5px;
}

.alert{
	background: #fff;
 	position: fixed;
    border-radius: 0px;
    width: 815px;
    height:600px;
    top: 0px;
    z-index: 10;
    padding:0;
}

.alert_content_div{
	float:left;
	text-align:left;
	font-size:14px;
	font-family:'宋体';
	font-weight: bold;
	height: 48px;
}

.alert_content_div_nobold{
	float:left;
	text-align:left;
	font-size:14px;
	font-family:'宋体';
	height: 49px;
}

.oper_btn{
	background-color:#6097fb;
	border:none;
	border-radius:0px;
	margin-top:14px;
	padding:0;
	font-size:12px;
	width:34px;
	height:21px
}

.weightAlert{
	  background: #fff;
    position: fixed;
    border-radius: 0px;
    width: 450px;
    height: 200px;
    top: 100px;
    z-index: 10;
    padding: 0;
}

.detail_upload{
	width:100px;
	height:40px;
	background-color:#6097fb;
	border:none;
	border-radius:0px;
	float:right;
	margin-right:50px;
}
