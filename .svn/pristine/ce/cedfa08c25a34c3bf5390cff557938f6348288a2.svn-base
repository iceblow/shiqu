@CHARSET "UTF-8";

body{
/* 	background:url(../imgs/love_background.png) no-repeat; */
/* 	background-size: cover; */
	font-size: 14px;
	font-family: "微软雅黑";
	padding: 0;
	margin: 0;
}

.top{
	width: 100%; 
	height: 49px;
	position: absolute;
	top: 0px;
	background: #ff8854;
	color: #fff;
}

.store_list{
	position:relative;
	width:100%;
}

.overflow_hidden {
    text-overflow: ellipsis;
    white-space: nowrap;
    overflow: hidden;
}

.select_calo{
	color: #ff4532;
}

.bottom_order{
	position: fixed;
	bottom: 0px;
	width: 100%;
	height: 60px;
	background: #756969;
	opacity: 0.8;
}

.fixedposition{
	background-color:#efefef;
	padding-left:3.75%;
	font-size:12px;
	padding-right:3.75%;
	height:34px;
	line-height:34px;
}

.storediv{
	width: 100%;
	height: 92px;
	border-bottom: 1px solid #e5e5e5;
}

.storemarginauto{
	width: 92%;
	height: 92px;
	margin: 0 auto;
}